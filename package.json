{
  "name": "cryptoweddings-contracts",
  "version": "0.0.1",
  "description": "smart contracts for blockchain marriage dapp",
  "scripts": {
    "lint:js": "eslint . --fix",
    "lint:contracts": "solium -d ./contracts/*/**",
    "start": "yarn ganache-cli -b 15 -d -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat'",
    "migrate:dev": "yarn truffle migrate --network dev --reset",
    "test": "truffle test",
    "clean:contracts": "rimraf build/contracts",
    "clean:install": "rimraf node_modules && yarn install",
    "build": "truffle compile"
  },
  "author": "Cody Lamson",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "ganache-cli": "^6.4.3",
    "openzeppelin-solidity": "^2.2.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "solium": "^1.2.4",
    "truffle": "^5.0.13"
  }
}
